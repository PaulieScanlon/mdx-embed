{
  "name": "docs",
  "version": "0.0.1",
  "description": "MDX Embed docs",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier --config ../.prettierrc.js --ignore-path ../.prettierignore --write \"**/*.{json,js,ts,tsx}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006 -s ./public --ci",
