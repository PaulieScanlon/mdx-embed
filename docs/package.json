    "url": "https://github.com/PaulieScanlon/mdx-embed/issues"
  },
  "homepage": "https://github.com/PaulieScanlon/mdx-embed#readme",
  "dependencies": {
    "@storybook/addon-docs": "^6.1.5",
    "@storybook/addon-links": "^6.1.5",
    "@storybook/addons": "^6.1.5",
    "@storybook/react": "^6.2.9",
    "@storybook/theming": "^6.1.5",
    "axios": "^0.21.1",
    "mdx-embed": "*",
    "react-docgen-typescript": "^1.20.5",
    "storybook-addon-mdx-embed": "*",
    "theme-ui": "^0.3.4"
  },
  "devDependencies": {
